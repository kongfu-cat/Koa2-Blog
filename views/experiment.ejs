            <option>xq-light</option>
            <option>yeti</option>
            <option>yonce</option>
            <option>zenburn</option>
        </select>
      <div class="editor css-editor">
        <textarea name="cssEditor" id="cssEditor"></textarea>
      </div>
      <div class="editor html-editor">
        <textarea name="htmlEditor" id="htmlEditor"></textarea>
      </div> 
      <div class="editor js-editor">
        <textarea name="jsEditor" id="jsEditor"></textarea>
      </div>
      <div class="result-iframe">
        <iframe id="resultIframe"></iframe>
      </div>
    </div>
    <div class="experiment-aside">
      <ul id="experimentTree" class="ztree experiment-tree"></ul>
      <!-- 新建/保存/修改/删除 -->
      <div class="experiment-tool">
        <% if (isDebug) { %>
          <button id="saveBtn" class="console-btn">保存</button>
          <button id="deleteBtn" class="console-btn">删除</button>
          <button id="newBtn" class="console-btn">新增</button>
        <% } %>
        <button id="genBtn" class="console-btn">生成</button>
      </div>
    </div>
    <div class="console-aside">
      <div class="console-btn-group">
          <button id="runBtn" class="console-btn">RUN</button>
          <button id="stopBtn" class="console-btn">STOP</button>
          <button id="autoBtn" class="console-btn">AUTO</button>
          <input type="number" value="1" id="autoInput">
      </div>
      <div class="console-history"></div>
      <div class="console-input">
          <span>> </span><input type="text" id="consoleInput">
      </div>
    </div>
    
    <footer class="footer">
      <%- include('include/footer') %>
    </footer>
